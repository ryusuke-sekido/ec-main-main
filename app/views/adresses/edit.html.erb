<head>
  <%= link_to '商品一覧', "/items" %>
</head>
<h1>配送先編集</h1>
<%= form_with model: @adress, url:adress_path(@adress.id), method: :patch do |f| %>
<h4>郵便番号</h4>
<%= f.text_field :postal_code %>
<h4>住所</h4>
<%= f.text_area :address %>
<h4>宛名</h4>
<%= f.text_area :name %>

<%= f.submit '更新' %>
<% end %>
